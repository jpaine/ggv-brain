{
  "version": 2,
  "builds": [
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/process",
      "dest": "/api/process.py"
    }
  ],
  "functions": {
    "api/process.py": {
      "maxDuration": 60
    }
  }
}

